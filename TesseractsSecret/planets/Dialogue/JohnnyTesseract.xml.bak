<DialogueTree xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/xen-42/outer-wilds-new-horizons/main/NewHorizons/Schemas/dialogue_schema.xsd">
	<NameField>Johnny Tesseract</NameField>

	<DialogueNode>
		<Name>MAIN</Name>
		<EntryCondition>DEFAULT</EntryCondition>
		<Dialogue>
			<Page>Well done! You found my secret.</Page>
		</Dialogue>
		
		<DialogueOptionsList>
			<DialogueOption>
				<Text>...Was that it?</Text>
				<DialogueTarget>1</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>

	<DialogueNode>
		<Name>1</Name>
		<Dialogue>
			<Page>Yup. You won. You had a very large brain and solved puzzles.</Page>
		</Dialogue>
		
		<DialogueOptionsList>
			<DialogueOption>
				<Text>Well, I AM pretty smart.</Text>
				<DialogueTarget>2</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>
	
	<DialogueNode>
		<Name>2</Name>
		<Dialogue>
			<Page>Smarter than me. This is what two weeks of this stuff'll do to ya.</Page>
		</Dialogue>
		
		<DialogueOptionsList>
			<DialogueOption>
				<Text>Better luck next time.</Text>
				<DialogueTarget>3</DialogueTarget>
			</DialogueOption>
		</DialogueOptionsList>
	</DialogueNode>
	
	<DialogueNode>
		<Name>3</Name>
		<Dialogue>
			<Page>Credits are behind me. Take a looksee.</Page>
			<Page>Ok - hay Buh - Buy</Page>
		</Dialogue>
	</DialogueNode>
	
</DialogueTree>